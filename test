<script>

    function resetNavPersonalisation() {
        function checkSessionStorageStatus() {
            try {
                sessionStorage.setItem('test', 1);
                sessionStorage.removeItem('test');
                return true;
            } catch(e) {
                return false;
            }
        }

        if (checkSessionStorageStatus()) {
            sessionStorage.setItem('personalisedNavNotification', 0);
        }
    }

    function HomepageTopNavUtils () {
      var isIpad = window.navigator && window.navigator.platform === 'iPad',
          isMobile = window.matchMedia && window.matchMedia("(max-width: 767px)"),
          isTablet = window.matchMedia && window.matchMedia("(min-width: 768px)"),
          active = 'is-active';

      var responsiveBodyClass = function() {
          var bodyClass = document.body.classList

          return isIpad || isMobile.matches ? bodyClass.add('has-onclick-nav') : bodyClass.remove('has-onclick-nav')

      }

      var desktopBodyClass = function() {
          var responsiveMenu = document.getElementById('js-menu'),
              menuCTA = document.getElementById('js-menu-cta')
          responsiveMenu.classList.remove(active)
          menuCTA.classList.remove(active)
      }

      isTablet.addListener(desktopBodyClass)
      isMobile.addListener(responsiveBodyClass)

      var handleClick = function () {
          event.preventDefault()

          if (isIpad || isMobile.matches) {
              var currentOpenMenu = document.getElementById('js-menu').getElementsByClassName(active);

              if(currentOpenMenu.length > 0) {
                  for (var i = 0; i < currentOpenMenu.length; i++) {
                      var el = currentOpenMenu[0]
                      if (el !== event.currentTarget) {
                          el.classList.remove(active)
                      } else if (el === event.currentTarget) {
                          return el.classList.remove(active)
                      }
                  }
              }
              return event.currentTarget.classList.add(active)
          }
      }

      var handleMenuClick = function () {
          event.preventDefault();
          event.currentTarget.classList.toggle(active)
          event.currentTarget.nextElementSibling.classList.toggle(active)
      }

      responsiveBodyClass()

      return {
          handleClick: handleClick,
          handleMenuClick: handleMenuClick
      }
  }
  var HomepageTopNavUtils = HomepageTopNavUtils()
</script>
